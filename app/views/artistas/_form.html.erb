<%= bootstrap_form_for(@artista) do |f| %>
  	<%= f.alert_message "Datos incorrectos, corrija los siguientes datos:" %>
  	<%= f.text_field :nombre %>
    <%= f.text_area :descripcion %>
    <%= f.date_select :fecha_origen %>
    <%= f.text_field :web %>
    <%= f.file_field :imagen%>
    <%= f.submit %>
<% end %>

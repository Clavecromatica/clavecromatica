<%= bootstrap_form_for(@cliente) do |f| %>
  <%= f.alert_message "Datos incorrectos, corrija los siguientes datos:" %>
    <%= f.text_field :nombre %>
  	<%= f.text_field :apellido_paterno %>
  	<%= f.text_field :apellido_materno %>
  	<%= f.text_field :direccion %>
  	<%= f.text_area :password%>
  	<%= f.text_area :email %>
    <%= f.submit %>
<% end %>
